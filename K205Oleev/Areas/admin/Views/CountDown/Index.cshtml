@model IEnumerable<CountDownLanguage>


<!-- DataTales Example -->
                    <div class="card shadow mb-4">
                        <div class="card-header py-3">
                            <h6 class="m-0 font-weight-bold text-primary"><a asp-action="create">Create</a></h6>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                    <thead>
                                        <tr>
                                            <th>Name</th>
                                            <th>Seo</th>
                                            <th>Count</th>
                                           
                                            <th></th>
                                            
                                        </tr>
                                    </thead>
                                    <tfoot>
                                        <tr>
                                            <th>Name</th>
                                            <th>Seo</th>
                                            <th>Count</th>
                                            
                                            <th></th>
                                            
                                        </tr>
                                    </tfoot>
                                    <tbody>
                                            @foreach (var item in Model)
                                             {
                                                <tr>
                                            <td>@item.Title</td>
                                            
                                            
                                            <td>@item.SEO</td>
                                            <td>@item.CountDown.Count</td>

                                            <td>
                                                    <a href="/admin/countdown/edit/@item.CountDown.Id" class="btn btn-warning">Edit</a>
                                            </td>
                                              



                                               </tr>
                                            }
                                        
                                 
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>